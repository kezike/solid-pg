{
  "name": "solid-pg",
  "version": "1.0.0",
  "description": "Solid Playground - Solid Single-Page Playground Application",
  "main": "src/index.js",
  "scripts": {
    "setup-cli": "chmod +x setup-cli.sh && ./setup-cli.sh",
    "setup-web": "chmod +x setup-web.sh && ./setup-web.sh",
    "build": "webpack --config ./app/webpack.config.js --mode production",
    "dev": "webpack --config ./app/webpack.config.js --mode development",
    "start": "npm run setup-web && webpack-dev-server --config ./app/webpack.config.js --mode development --hot"
  },
  "repository": {
