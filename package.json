    "oidc-auth-manager": "^0.16.1",
    "requirejs": "^2.3.5",
    "solid-app-set": "^1.0.0",
    "solid-client": "^0.24.3",
    "standard": "^10.0.3",
    "style-loader": "^0.19.1",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
